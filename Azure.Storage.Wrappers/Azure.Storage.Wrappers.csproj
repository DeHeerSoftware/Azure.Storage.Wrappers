<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <Version>0.0.1</Version>
    <Authors>De Heer Software</Authors>
    <Company>De Heer Software</Company>
    <Description>Provides interfaces to make Azure Storage unit testable</Description>
    <PackageLicenseUrl>https://github.com/DeHeerSoftware/Azure.Storage.Wrappers/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/DeHeerSoftware/Azure.Storage.Wrappers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DeHeerSoftware/Azure.Storage.Wrappers</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
    <PackageReleaseNotes>version number update to publish to nuget.org</PackageReleaseNotes>
    <PackageId>AzureStorage.Wrappers</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="WindowsAzure.Storage" Version="8.2.0" />
  </ItemGroup>

</Project>
